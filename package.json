{
  "name": "graph_ql_apollo_client",
  "version": "1.0.0",
  "description": "GraphQL client on Apollo",
  "main": "lib/index.js",
  "scripts": {
    "build": "webpack",
    "start": "cross-env PATH=\"../node_modules/.bin:$PATH\" BACKEND=dev47 webpack-dev-server --port 2500 --content-base ./build",
    "babelrc": "cross-env PATH=\"../node_modules/.bin:$PATH\" dep-mngr-webpack",
    "clear": "rm -rf node_modules; rm -rf lib; rm -rf public",
    "compile": "cross-env PATH=\"../node_modules/.bin:$PATH\" babel src --out-dir lib --ignore *.test.js,__mocks__",
    "eslintrc": "cross-env PATH=\"../node_modules/.bin:$PATH\" dep-mngr-eslint",
    "lint": "cross-env PATH=\"../node_modules/.bin:$PATH\" eslint ./src && echo \"eslint: ok!\"",
    "managers": "npm install --prefix ../ dep-mngr-webpack dep0-mngr-eslint",
    "prepublish": "npm run lint && npm run test && npm run compile",
    "rc": "npm run babelrc; npm run eslintrc",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Valentin1918/graph_ql_apollo_client.git"
  },
  "keywords": [],
  "author": "grekulov.valentyn@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Valentin1918/graph_ql_apollo_client/issues"
  },
  "dependencies": {
    "apollo-client": "^2.2.8",
    "graphql": "^0.13.2",
    "prop-types": "^15.5.10",
    "react-apollo": "^2.1.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "4.1.5",
    "core-components": "^1.0.270",
    "cross-env": "^5.1.4",
    "fe-redux-core": "^1.0.6",
    "frontend-api-client": "^1.0.12",
    "frontend-webpack-config": "^1.3.0",
    "pdf-render-component": "^1.1.60",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-logger": "^3.0.6",
    "safe-monada": "^2.3.2"
  },
  "homepage": "https://github.com/Valentin1918/graph_ql_apollo_client#readme"
}
